{
  "parser": "@typescript-eslint/parser",
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins":["@typescript-eslint", "react", "prettier"],
    "settings":  {
      "react":  {
        "version":  "detect"
      }
    }
}